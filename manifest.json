{
  "manifest_version": 3,
  "name": "Leetcode Extension",
  "version": "1.0",
  "permissions": ["scripting", "activeTab"],
  "content_scripts": [
    {
      "matches": ["https://leetcode.com/*"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["assets/images/*", "assets/audio/*"],
      "matches": ["https://leetcode.com/*"]
    }
  ]
}
